flick fib(n: ant) ant {
  spider (n <= 1){
    return 1;
  }web{
    return fib(n-1) + fib(n-2);
  }
  end_spider
  //return 0;
}

flick main(){
  fly(bee ant i = 0; i < 21; i++){
    spray(fib(i));
  }
}

main();